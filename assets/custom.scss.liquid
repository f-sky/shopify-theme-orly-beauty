	margin-bottom:20px;}

.prduct_review .spr-header-title {color: #340800;
    font-size: 27px;
    font-family: "AvenirNext","GothamHTFBook","Helvetica","Arial",sans-serif;
    font-weight: 700;
    text-transform: capitalize;
    line-height: normal;
    margin-bottom: 30px;}

.prduct_review .actions-newreview {float: none;
    display: block;
    color: #24c0ee;
    font-size: 15px;
    line-height: 19.5px;
    font-family: "AvenirNext";
    cursor: pointer;}

.review_popup {position: fixed;
    background: rgba(0,0,0,0.4);
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    width: 100%;
    z-index: 9;
    opacity: 0;
  	padding-top:50px;
    visibility: hidden;}

.review_popup.show_review {opacity:1;
	visibility:visible;}

.review_scroll .header-top {z-index:0;}

.review_content {position:absolute;
	width: 80%;
    max-width: 840px;
    margin: auto;
	background:#fff;}

.review_popup #shopify-product-reviews, .review_close {overflow: visible;
    position: relative;
    -webkit-transition: all ease .4s;
    -moz-transition: all ease .4s;
    -o-transition: all ease .4s;
    transition: all ease .4s;
    opacity: 0;
    -ms-filter: progid:DXImageTransform.Microsoft.Alpha(Opacity=0);
    filter: alpha(opacity=0);
    -webkit-transform: translateZ(0) scale(0.8);
    -moz-transform: translateZ(0) scale(0.8);
    -o-transform: translateZ(0) scale(0.8);
    transform: translateZ(0) scale(0.8);}

.show_review #shopify-product-reviews {transform:none;
	opacity:1;
	margin: auto;
    background: #fff;
    max-width: 840px;}

.show_review .review_close {margin:auto;
    background: transparent;
    transform:none;
	opacity:1;
	margin: auto;
    max-width: 840px;
    overflow: hidden;
	position: absolute;
    right: 0;
    left: 0;
    z-index: 9;}

.review_close span {font: 20px Arial, Helvetica, sans-serif;
    vertical-align: middle;
    color: #fff;
    background: #999;
    margin: 5px;
    line-height: 24px;
    border-radius: 100%;
    padding: 0px 8px;
    cursor: pointer;
    float: right;
    width: 26px;
    height: 26px;
	transition:all 0.3s ease-in-out;
	-moz-transition:all 0.3s ease-in-out; 
	-o-transition:all 0.3s ease-in-out; 
	-webkit-transition:all 0.3s ease-in-out; 
	-ms-transition:all 0.3s ease-in-out;}

.review_close span:hover {background:#000;}

.review_popup .spr-summary {display:none;}

.review_popup .spr-container {padding: 0;
    border: none;}

.review_popup .spr-header {padding:20px 10px;} 

.review_popup .spr-header .spr-header-title {margin:auto;}

.review_popup .spr-content {background: #f7f7f7;
    padding: 10px;}

.review_popup .spr-form {margin: auto;
    padding: 0;
    border: none;}

.review_popup .spr-form-title {color: #340800;
    font-size: 27px;
    font-family: "AvenirNext","GothamHTFBook","Helvetica","Arial",sans-serif;
    font-weight: 700;
